{
  "name": "@saascannon/auth",
  "version": "0.0.2",
  "description": "Node.js Library for authorizing requests to your apis using Saascannon credentials",
  "type": "module",
  "scripts": {
    "test": "jest",
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "keywords": [
    "Saascannon"
  ],
  "author": "Saascannon",
  "license": "MIT",
  "dependencies": {
    "jose": "^5.8.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.0"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/saascannon/saascannon-node-auth",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saascannon/saascannon-node-auth.git"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/lib.d.ts",
        "default": "./dist/lib.js"
      },
      "require": {
        "types": "./dist/lib.d.cts",
        "default": "./dist/lib.cjs"
      }
    },
    "./express": {
      "import": {
        "types": "./dist/express.d.ts",
        "default": "./dist/express.js"
      },
      "require": {
        "types": "./dist/express.d.cts",
        "default": "./dist/express.cjs"
      }
    },
    "./package.json": "./package.json"
  }
}
